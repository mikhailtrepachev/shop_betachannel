<MudDialog>
    <DialogContent>
        <MudTextField T="string" @bind-value="PhoneNumber" Label="Your phone number"></MudTextField>
        <MudTextField T="string" @bind-value="Details" Label="Details"></MudTextField>
        <MudSelect T="string" @bind-value="Location" Label="Choose your location" AnchorOrigin="Origin.BottomCenter">
            <MudSelectItem Value="@("Germany")"></MudSelectItem>
            <MudSelectItem Value="@("Czech Republic")"></MudSelectItem>
            <MudSelectItem Value="@("Poland")"></MudSelectItem>
        </MudSelect>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Primary" OnClick="UploadOrderToDatabase">Ok</MudButton>
    </DialogActions>
</MudDialog>

 @code {

     [CascadingParameter]
     MudDialogInstance MudDialog { get; set; } = new();
 
     void Submit() => MudDialog.Close(DialogResult.Ok(true));
     void Cancel() => MudDialog.Cancel();
 }